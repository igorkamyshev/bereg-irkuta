{
  "name": "nodejs-sdk",
  "version": "2.1.3",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "build": "parcel build front/index.js --out-dir public",
    "dev": "yarn nodemon app.js",
    "lint": "eslint *.js",
    "ci": "yarn s lint",
    "s": "yarn soda"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "compression": "^1.7.4",
    "debug": "~3.1.0",
    "errorhandler": "~1.5.0",
    "express": "~4.16.1",
    "lodash": "^4.17.11",
    "method-override": "~2.3.10",
    "morgan": "~1.9.0",
    "prismic-dom": "~2.0.4",
    "prismic-javascript": "~1.4.0",
    "pug": "~2.0.0-rc.2",
    "request": "~2.83.0",
    "serve-favicon": "~2.4.3",
    "turbolinks": "^5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/prismicio/nodejs-sdk.git"
  },
  "devDependencies": {
    "@solid-soda/scripts": "^1.6.3",
    "nodemon": "^1.17.3",
    "parcel-bundler": "^1.12.3"
  },
  "engines": {
    "node": ">=4.8.1",
    "npm": ">=2.15.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn soda lint-staged",
      "commit-msg": "yarn soda commitlint"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
